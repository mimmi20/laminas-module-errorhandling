{
  "name": "mimmi20/laminas-module-errorhandling",
  "description": "Provides a Log Listener for Laminas MVC Applications",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/laminas-module-errorhandling/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminas-module-errorhandling",
  "support": {
    "issues": "https://github.com/mimmi20/laminas-module-errorhandling/issues",
    "source": "https://github.com/mimmi20/laminas-module-errorhandling"
  },
  "require": {
    "php": "^8.1.0",
    "psr/log": "^3.0.0",
    "laminas/laminas-mvc": "^3.6.1"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.0",
    "laminas/laminas-component-installer": "^3.3.0",
    "laminas/laminas-eventmanager": "^3.10.0",
    "laminas/laminas-modulemanager": "^2.14.0",
    "laminas/laminas-servicemanager": "^3.21.0",
    "mimmi20/coding-standard": "^5.1.10",
    "nikic/php-parser": "^v4.16.0",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.28",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.13",
    "phpunit/phpunit": "^10.3.1",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^0.17.10",
    "vimeo/psalm": "^5.14.1"
  },
  "suggest": {
    "laminas/laminas-modulemanager": "Required for Module Integration",
    "laminas/laminas-servicemanager": "Required for the Factories"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\ErrorHandling\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\ErrorHandling\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "laminas/laminas-component-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "extra": {
    "laminas": {
      "component": "Mimmi20\\ErrorHandling"
    }
  }
}
